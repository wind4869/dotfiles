[user]
	name = wind4869
	email = windv587@gmail.com
[credential]
	helper = osxkeychain
[commit]
	template = /Users/wind/.gitmessage
[core]
	editor = subl -n -w
[push]
	default = simple
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
	st = status
	ci = commit
	co = checkout
	br = branch
[color]
	ui = true
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
